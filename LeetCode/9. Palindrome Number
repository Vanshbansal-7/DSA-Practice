class Solution {
    public boolean isPalindrome(int x) {
        int remainder = 0;
        int reversed =0;
        int num=x;
        while(x>0){
            remainder=x%10;
            reversed=reversed*10+remainder;
            x=x/10;
        }
        if (num==reversed){
            return true;
        
        }
        else{
            return false;
        }

    }
}
