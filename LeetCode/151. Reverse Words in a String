class Solution {
    public String reverseWords(String s) {
        String[] a=s.trim().split("\\s+");
        int n=a.length;
        for(int i=0;i<n/2;i++){
            String temp=a[i];
            a[i]=a[n-1-i];
            a[n-1-i]=temp;
        }
        return String.join(" " , a);
        
    }
}
