class Solution {
    public int missingNumber(int[] nums) {
        int n=nums.length;
        Arrays.sort(nums);
        int num=0;
        for (int i=0;i<n;i++){
            if (nums[i]==num){
                num++;
            }
            else{
                return num;
            
            }
            
        }
        return num;
    }
}
